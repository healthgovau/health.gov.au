  "private": true,
  "//": "The postinstall script is needed to work-around this Drupal core bug: https://www.drupal.org/node/2329453",
  "scripts": {
    "postinstall": "npm rebuild node-sass && find node_modules/ -name '*.info' -type f -delete",
    "test": "gulp",
    "pancake": "node node_modules/@gov.au/pancake/bin/pancake"
  },
  "pancake": {
    "auto-save": true,
    "plugins": true,
    "ignore": [],
    "css": {
      "minified": true,
      "modules": false,
      "browsers": [
        "last 2 versions",
        "ie 8",
        "ie 9",
        "ie 10"
      ],
      "location": "css/",
      "name": false
    },
    "sass": {
      "modules": false,
      "location": "sass/vendors/",
      "name": "uikit.scss"
    },
    "js": {
      "minified": true,
      "modules": false,
      "location": "pancake/js/",
      "name": "pancake.js"
    },
    "react": {
      "location": "pancake/react/"
    }
  }
